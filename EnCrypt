local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/FlingerM/Main/main/library.lua"))() local Window = Library.createWindow("GG HUB", "GG HUB", true) local Main = Window.createTab("Main") local MainSection = Main.createSection("Main", false) local Player = game.Players.LocalPlayer local Character = Player.Character local Mouse = Player:GetMouse() local Humanoid = Character:WaitForChild("Humanoid") local Variables = { Variable1 = false, Core = { Progress_Down = false, PcallDebug = false, MethodDebug = "While", CurrentTime = 0, DevBar = false, }, } local Reset = function() for _, i in pairs(Variables) do if typeof(i)=="table" then if i~="Core" then for _i, j in pairs(i) do if typeof(j)=="boolean" then j=not j else j=nil end end end if typeof(i)=="boolean" then i=not j else i=nil end end end Variables.Core.Progress_Down = true end local MasterTween = function(Thing, Goal) local Tween if Goal and Thing then Tween = game:GetService("TweenService"):Create(Thing, TweenInfo.new(1, Enum.EasingStyle.Bounce), Goal) else warn("Item or Goal not setted, did you forget?") end return Tween end Variables.add = function(Name, Value) if Name and Value then Variables[Name]=Value else warn("Name or value not assigned") end end Variables.get = function(Name) return Variables[Name] end Variables.set = function(Name, Value) Variables[Name]=Value end Variables.getintable = function(Name, TableName) return Variables[TableName][Name] end Variables.setintable = function(Name, TableName, Value) local startPos, endPos = string.find(TableName, "Core") if not startPos then Variables[TableName][Name]=Value else warn("Name of table: "..string.sub(TableName, startPos, endPos).." Have name of CoreTables") end end Variables.addtotable = function(Name, TableName, Value) if Name and Value and TableName then Variables[TableName][Name]=Value else warn("Name, TableName or value not assigned") end end if Variables["Core"]["DevBar"] then local DevTab = Window.createTab("ForScripters") local DevSection = Main.createSection("Requires", false) DevSection.createButton("Dex", function() loadstring(game:GetObjects("rbxassetid://2180084478")[1].Source)() end) DevSection.createButton("Roblox spy", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))() end) end local d=MainSection.createTextBox("Decoder", "load()") local dec=function(t)local o=""for n in string.gmatch(t,"%d+")do local r=(tonumber(n)/2) local a=string.char(r)if a then o=o..a end end return o end MainSection.createButton("return", function() loadstring(dec(d.getText())) d.clearText() end) local e=MainSection.createTextBox("Encoder", "crypt()") local enc=function(t)local o=""for n=1,#t do local r=string.byte(string.sub(t,n,n))o=o..math.round(tostring((r*2)))..string.char(math.random(64, 72))end return o end MainSection.createButton("return [2]", function() setclipboard(enc(e.getText()))() e.clearText() end) local Code = function() end Main.createButton("Remove GUI", function() Reset() game:GetService('StarterGui'):SetCore('SendNotification', { Title = 'Thanks for using!', Text = 'By HOKJHa', Duration = 5 }) Window.deleteWindow("GG HUB") end) while wait(Variables.Core.CurrentTime) do if not Variables.Core.Progress_Down then if Variables.Core.PcallDebug then if Variables.Core.MethodDebug == "While" then pcall(Code) elseif Variables.Core.MethodDebug == "One-Time" then Variables.Core.CurrentTime = math.huge end else if Variables.Core.MethodDebug == "While" then Code() elseif Variables.Core.MethodDebug == "One-Time" then Variables.Core.CurrentTime = math.huge end end end end 
