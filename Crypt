local Library=loadstring(game:HttpGet('https://raw.githubusercontent.com/FlingerM/Main/main/library.lua'))()local Window=Library.createWindow('GG','GG',true)local Main=Window.createTab('Main')local MainSection=Main.createSection('Main',false)local Player=game.Players.LocalPlayer local Character=Player.Character local Mouse=Player:GetMouse() local Humanoid=Character:WaitForChild('Humanoid')local Variables={Variable1=false,Core={Progress_Down=false,PcallDebug=false,MethodDebug='While',CurrentTime=0,DevBar=false}}local Reset=function()for _,i in pairs(Variables)do if typeof(i)=='table' then if i~='Core'then for _i,j in pairs(i)do if typeof(j)=='boolean'then j=not j else j=nil end end end if typeof(i)=='boolean'then i=not j else i=nil end end end Variables.Core.Progress_Down=true end if Variables['Core']['DevBar']then local DevTab=Window.createTab('ForScripters')local DevSection=Main.createSection('Requires',false)DevSection.createButton('Dex',function()loadstring(game:GetObjects('rbxassetid://2180084478')[1].Source)()end)DevSection.createButton('Roblox spy',function()loadstring(game:HttpGet('https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua'))()end)end local Code=function()if Variables.Variable1 then end end Main.createButton('Remove GUI',function()Reset()game:GetService('StarterGui'):SetCore('SendNotification',{Title='Thanks for using!',Text='By HOKJHa',Duration=5})Window.deleteWindow('GG')end)MainSection.createToggle('Test toggle',false,function(callback)Variables.Variable1=callback end)while wait(Variables.Core.CurrentTime)do if not Variables.Core.Progress_Down then if Variables.Core.PcallDebug then if Variables.Core.MethodDebug=='While' then pcall(Code)elseif Variables.Core.MethodDebug=='One-Time' then Variables.Core.CurrentTime=math.huge end else if Variables.Core.MethodDebug=='While' then Code()elseif Variables.Core.MethodDebug=='One-Time' then Variables.Core.CurrentTime=math.huge end end end end
